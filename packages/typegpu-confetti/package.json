{
  "name": "typegpu-confetti",
  "version": "0.1.0",
  "description": "Customizable confetti animation component for React and React Native, running on the GPU",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./react-native": "./src/react-native/index.ts",
    "./react": "./src/react/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.mjs",
        "import": "./dist/index.mjs",
        "default": "./dist/index.cjs"
      },
      "./react-native": {
        "types": "./dist/react-native/index.d.ts",
        "module": "./dist/react-native/index.mjs",
        "import": "./dist/react-native/index.mjs",
        "default": "./dist/react-native/index.cjs"
      },
      "./react": {
        "types": "./dist/react/index.d.ts",
        "module": "./dist/react/index.mjs",
        "import": "./dist/react/index.mjs",
        "default": "./dist/react/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsx prepack.mjs",
    "prepublishOnly": "pnpm run build",
    "test": "vitest"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhawryluk/typegpu-confetti.git"
  },
  "keywords": [
    "confetti",
    "javascript",
    "react-native",
    "webgpu",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/mhawryluk/typegpu-confetti/issues"
  },
  "dependencies": {
    "@typegpu/noise": "^0.1.0",
    "typegpu": "^0.6.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-wgpu": "*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/testing-library__react": "^10.2.0",
    "@webgpu/types": "^0.1.54",
    "execa": "^9.6.0",
    "jsdom": "^26.1.0",
    "react": "^19.1.0",
    "react-native": "^0.79.3",
    "react-native-wgpu": "^0.1.23",
    "remeda": "^2.21.2",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3",
    "unbuild": "^3.5.0",
    "unplugin-typegpu": "^0.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
