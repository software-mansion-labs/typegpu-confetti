{
  "name": "typegpu-confetti",
  "version": "0.1.0",
  "description": "Customizable confetti animation component for React and React Native, running on the GPU",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./react-native": "./src/react-native/index.ts",
    "./react": "./src/react/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.mjs",
        "import": "./dist/index.mjs",
        "default": "./dist/index.cjs"
      },
      "./react-native": {
        "types": "./dist/react-native/index.d.ts",
        "module": "./dist/react-native/index.mjs",
        "import": "./dist/react-native/index.mjs",
        "default": "./dist/react-native/index.cjs"
      },
      "./react": {
        "types": "./dist/react/index.d.ts",
        "module": "./dist/react/index.mjs",
        "import": "./dist/react/index.mjs",
        "default": "./dist/react/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "node prepack.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhawryluk/typegpu-confetti.git"
  },
  "keywords": [
    "confetti",
    "javascript",
    "react-native",
    "webgpu",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/mhawryluk/typegpu-confetti/issues"
  },
  "dependencies": {
    "@typegpu/noise": "0.7.*",
    "typegpu": "0.7.*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-wgpu": "*"
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.64",
    "execa": "^9.6.0",
    "react": "^19.1.1",
    "react-native": "^0.79.3",
    "react-native-wgpu": "^0.2.1",
    "remeda": "^2.30.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "unplugin-typegpu": "^0.2.2"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
