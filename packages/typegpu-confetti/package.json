{
  "name": "typegpu-confetti",
  "version": "0.0.2",
  "description": "Customizable confetti animation component, running on the GPU",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./react-native": "./src/react-native/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      },
      "./react-native": {
        "types": "./dist/react-native/index.d.ts",
        "module": "./dist/react-native/index.js",
        "import": "./dist/react-native/index.js",
        "default": "./dist/react-native/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "unbuild && cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhawryluk/typegpu-confetti.git"
  },
  "keywords": ["confetti", "javascript", "react-native", "webgpu"],
  "bugs": {
    "url": "https://github.com/mhawryluk/typegpu-confetti/issues"
  },
  "dependencies": {
    "react": "18.3.1",
    "typegpu": "^0.5.6",
    "@typegpu/noise": "^0.0.6"
  },
  "peerDependencies": {
    "react-native-wgpu": "^0.1.23",
    "react-native": "0.76.9"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@webgpu/types": "^0.1.54",
    "unbuild": "^3.5.0",
    "unplugin-typegpu": "^0.1.0",
    "react-native-wgpu": "^0.1.23",
    "react-native": "0.76.9"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
